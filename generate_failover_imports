#!/usr/bin/env python

infile = sys.argv[1]
outfile = sys.argv[2]

with open(infile) as fin:
    with open(outfile, 'w') as fout:
        for line in fin:
            fout.write('from contextlib import suppress\n')
            fout.write('with suppress(ImportError, ModuleNotFoundError):\n')
            fout.write('    ' + line)
